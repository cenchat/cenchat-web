{{#if model.title}}
  <title>{{model.title}}</title>
  <meta property="og:title" content="{{model.title}}">
  <meta name="twitter:title" content="{{model.title}}">
{{else}}
  <title>Cenchat</title>
{{/if}}

{{#if (eq model.description '&nbsp;')}}
  <meta name="description" content="&nbsp;">
  <meta property="og:description" content="&nbsp;">
  <meta name="twitter:description" content="&nbsp;">
{{else if model.description}}
  <meta name="description" content="{{model.description}}">
  <meta property="og:description" content="{{model.description}}">
  <meta name="twitter:description" content="{{model.description}}">
{{/if}}

{{#if model.image}}
  <meta property="og:image" content="{{model.image}}">
  <meta name="twitter:image" content="{{model.image}}">
{{else}}
  <meta property="og:image" content="https://firebasestorage.googleapis.com/v0/b/cenchat-prod.appspot.com/o/assets%2Fimages%2Flogos%2Fcenchat%2Fcenchat_white_1200.png?alt=media&token=c1aef38d-82d5-4bcf-a4e5-980e02b73bc1">
  <meta name="twitter:image" content="https://firebasestorage.googleapis.com/v0/b/cenchat-prod.appspot.com/o/assets%2Fimages%2Flogos%2Fcenchat%2Fcenchat_white_1200.png?alt=media&token=c1aef38d-82d5-4bcf-a4e5-980e02b73bc1">
{{/if}}

{{#if model.type}}
  <meta property="og:type" content="{{model.type}}">
{{else}}
  <meta property="og:type" content="website">
{{/if}}

{{#if model.url}}
  <link rel="canonical" href="{{model.url}}">
  <meta property="og:url" content="{{model.url}}">
{{/if}}

{{#if model.author}}
  <meta property="author" content="{{model.author}}">
{{/if}}

